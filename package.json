{
  "name": "babelone-seed",
  "version": "0.0.2",
  "description": "Seed project for implementing backbonejs with es6 and babel transpiler",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "mocha ./src/**/*_test.js -R nyan",
    "lint": "globstar --node -- eslint ./src/**/*.js -f node_modules/eslint-friendly-formatter",
    "prebuild:scripts": "mkdirp ./dist/scripts && npm test",
    "build:scripts": "browserify ./src/app.js -d -t [babelify --sourceMapRelative .] | exorcist ./dist/scripts/app.min.js.map > ./dist/scripts/app.min.js",
    "prebuild:styles": "mkdirp ./dist/styles",
    "build:styles": "stylus -u autoprefixer-stylus -c ./src/app.styl --out ./dist/styles/app.min.css",
    "build:markup": "",
    "server": "",
    "livereload": ""
  },
  "keywords": [
    "Babel",
    "es6",
    "BackboneES6"
  ],
  "author": "Marco Romero",
  "license": "MIT",
  "dependencies": {
    "backbone": "~1.1.2",
    "react": "~0.13.0-rc2"
  },
  "devDependencies": {
    "autoprefixer-stylus": "^0.6.0",
    "babel-eslint": "~3.1.1",
    "chai": "~2.2.0",
    "cssmin": "^0.4.3",
    "eslint": "~0.20.0",
    "eslint-friendly-formatter": "~1.0.5",
    "eslint-plugin-react": "~2.2.0",
    "exorcist": "~0.3.0",
    "globstar": "~1.0.0",
    "gulp": "~3.8.11",
    "gulp-connect": "~2.2.0",
    "gulp-jade": "~1.0.0",
    "mkdirp": "^0.5.0",
    "mocha": "~2.2.4",
    "sinon": "~1.14.1",
    "stylus": "^0.51.1",
    "vinyl-source-stream": "~1.1.0"
  }
}
